  "name": "hellofresh-server",
  "version": "1.0.0",
  "description": "HelloFreshGO - Front-end Exercise =============================================",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev:client": "npm run start --prefix client",
